public class RelationalLogicalOperators {
    public static void main(String args[]){
        int a=10;
        int b=28;
       // System.out.println(a>b);
        System.out.println(a==b || a<b);
        System.out.println(a==b || a>b);
        System.out.println(a!=b);
        System.out.println(a<=b);
        System.out.println(a>=b);
        System.out.println(!(a>b));
    }
    
}

package array;

//import com.sun.xml.internal.fastinfosest.tools.XML_SAX_StAX_FI;

import java.util.Scanner;

public class largestelement {
    public static void main(String[] args)
    {
        int[] arr=new int[5];
        System.out.println("Enter array element :");
        for(int i=0;i<arr.length;i++)
        {
            Scanner a=new Scanner(System.in);
            arr[i] =a.nextInt();

        }
        int maximum=0;
        for(int i=0;i<arr.length;i++)
        {
            if(maximum<arr[i]){
                maximum=arr[i];
            }
        }
        System.out.println("greater element in an array :"+ maximum);
    }
    
}
// enter array element: 1,2,3,5
// largest element in an array: 5
